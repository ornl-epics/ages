######################################################
#Handle the Log in for admin user modes
######################################################
record (bo, "BL11A:SE:AGES:PLCAGESADMINLOGIN")
{
       field (DESC, "ADMIN USER LOG IN")
       field (ZNAM, "User Mode ON")
       field (ONAM, "ADMIN Mode ON")
       field (DTYP, "EtherIP")
       field (OUT, "@plc1 E01D01[2] B 0")
       field (SCAN, ".5 second")
       info(archive, "Monitor, 00:00:01, VAL")
       field (OSV, "MINOR")

}


######################################################
#Invert the ADMINLOGIN Value
######################################################
record(calc, "BL11A:SE:AGES:PLCAGESADMINLOGINInv") {
  field(DESC, "Invert ADMIN LOG IN BO")	
  field(INPA, "BL11A:SE:AGES:PLCAGESADMINLOGIN CP NMS")
  field(CALC, "A>0?0:1")
  info(archive, "Monitor, 00:00:01, VAL")


}



