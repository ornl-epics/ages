#Alarm Handler ALicat AGES
#####################################################################



record(calc, "BL11A:SE:AGES:GlobalAlicatAlarm")
{
    field(INPA, "BL11A:SE:AlicatAges:AMassFlow.SEVR CPP")
    field(INPB, "BL11A:SE:AlicatAges:BMassFlow.SEVR CPP")
    field(INPC, "BL11A:SE:AlicatAges:CMassFlow.SEVR CPP")
    field(INPD, "BL11A:SE:AlicatAges:DMassFlow.SEVR CPP")
    field(INPE, "BL11A:SE:AlicatAges:EMassFlow.SEVR CPP")
    field(INPF, "BL11A:SE:AlicatAges:FMassFlow.SEVR CPP")
    field(INPG, "BL11A:SE:AlicatAges:GMassFlow.SEVR CPP")
    field(INPH, "BL11A:SE:AGES:GlobalAlicatAlarmDisable CPP")
    field(CALC, "((A>0)|(B>0)|(C>0)|(D>0)|(E>0)|(F>0)|(G>0))&H")
    field(HIHI, "1")
    field(LOLO, "0")
    field(HHSV, "MAJOR")
    field(LLSV, "NO_ALARM")
    field(PINI, "YES")
    info(archive, "Monitor, 00:00:01, VAL")

}




record(bi, "BL11A:SE:AGES:GlobalAlicatAlarmDisable")
{
        field(DESC, "Summary Alarm Enable")
        field(ZNAM, "Disabled")
        field(ONAM, "Enabled")
        field(ASG, "ALWAYS")
        field(PINI, "YES")
        info(archive, "Monitor, 00:00:01, VAL")
        field(ZSV, "MINOR")
        field(OSV, "NO_ALARM")

}

